{
    "rewrites": [
      {
        "source": "/blog",
        "destination": "https://blog.samiralibabic.com"
      },
      {
        "source": "/blog/:path*",
        "destination": "https://blog.samiralibabic.com/:path*"
      }
    ],
    "headers": [
      {
        "source": "/blog/:path*",
        "headers": [
          { "key": "X-Frame-Options", "value": "ALLOWALL" },
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ]
  }